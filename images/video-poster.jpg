<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="675" viewBox="0 0 1200 675">
  <!-- Background -->
  <rect width="1200" height="675" fill="#212121" />
  
  <!-- Gradient Overlay -->
  <rect width="1200" height="675" fill="url(#grad1)" opacity="0.8" />
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6200ee;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#03dac6;stop-opacity:0.6" />
    </linearGradient>
  </defs>
  
  <!-- Play Button -->
  <circle cx="600" cy="337.5" r="80" fill="#ffffff" fill-opacity="0.9" />
  <polygon points="580,300 580,375 640,337.5" fill="#6200ee" />
  
  <!-- Title -->
  <text x="600" y="500" font-family="Arial" font-size="40" fill="#ffffff" text-anchor="middle">DMac Demo</text>
  <text x="600" y="550" font-family="Arial" font-size="24" fill="#ffffff" text-anchor="middle">See the AI Agent Swarm in Action</text>
  
  <!-- Agent Icons -->
  <g transform="translate(300, 200) scale(0.5)">
    <!-- Cody -->
    <circle cx="100" cy="100" r="90" fill="#bb86fc" fill-opacity="0.7" />
    <rect x="60" y="50" width="80" height="70" rx="10" fill="#6200ee" fill-opacity="0.7" />
    <circle cx="80" cy="80" r="10" fill="#03dac6" />
    <circle cx="120" cy="80" r="10" fill="#03dac6" />
    <rect x="75" y="100" width="50" height="5" rx="2" fill="#03dac6" />
  </g>
  
  <g transform="translate(500, 200) scale(0.5)">
    <!-- Perry -->
