{
    "include": [
        "."
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.ipynb_checkpoints"
    ],
    "ignore": [
        "**/node_modules",
        "**/__pycache__",
        "**/.ipynb_checkpoints"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "venvPath": ".",
