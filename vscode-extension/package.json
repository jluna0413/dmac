{
  "name": "dmac-vscode",
  "displayName": "DMac AI Agent Swarm",
  "description": "VS Code extension for DMac AI Agent Swarm",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Programming Languages"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dmac.startAgent",
        "title": "DMac: Start AI Agent"
      },
      {
        "command": "dmac.chat",
        "title": "DMac: Open Chat Interface"
      },
      {
        "command": "dmac.generateCode",
        "title": "DMac: Generate Code"
      },
      {
        "command": "dmac.explainCode",
        "title": "DMac: Explain Selected Code"
      },
      {
        "command": "dmac.optimizeCode",
        "title": "DMac: Optimize Selected Code"
      },
      {
        "command": "dmac.debugCode",
        "title": "DMac: Debug Selected Code"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dmac-sidebar",
          "title": "DMac",
          "icon": "resources/dmac-icon.svg"
        }
      ]
    },
    "views": {
      "dmac-sidebar": [
        {
          "id": "dmacAgents",
          "name": "AI Agents",
          "icon": "resources/dmac-icon.svg"
        },
        {
          "id": "dmacChat",
          "name": "Chat",
          "icon": "resources/dmac-icon.svg"
        },
        {
          "id": "dmacTasks",
          "name": "Tasks",
          "icon": "resources/dmac-icon.svg"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "dmac.explainCode",
          "when": "editorHasSelection",
          "group": "dmac"
        },
        {
          "command": "dmac.optimizeCode",
          "when": "editorHasSelection",
          "group": "dmac"
        },
        {
          "command": "dmac.debugCode",
          "when": "editorHasSelection",
          "group": "dmac"
        }
      ]
    },
    "configuration": {
      "title": "DMac",
      "properties": {
        "dmac.serverUrl": {
          "type": "string",
          "default": "http://localhost:8080",
          "description": "URL of the DMac server"
        },
        "dmac.defaultModel": {
          "type": "string",
          "default": "gemma3:12b",
          "description": "Default AI model to use"
        },
        "dmac.enableAutoSuggestions": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic code suggestions"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "socket.io-client": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jluna0413/dmac.git"
  },
  "publisher": "jluna0413",
  "author": "Jonathan Luna",
  "license": "MIT"
}