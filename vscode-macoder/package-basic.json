{
  "name": "vscode-macoder",
  "displayName": "MaCoder",
  "description": "MaCoder VS Code Extension",
  "version": "0.3.0-alpha",
  "publisher": "dmac",
  "repository": {
    "type": "git",
    "url": "https://github.com/jluna0413/dmac.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:macoder.startChat",
    "onCommand:macoder.generateCode",
    "onCommand:macoder.explainCode",
    "onCommand:macoder.refactorCode",
    "onCommand:macoder.showInfo"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "macoder.startChat",
        "title": "MaCoder: Start Chat"
      },
      {
        "command": "macoder.generateCode",
        "title": "MaCoder: Generate Code"
      },
      {
        "command": "macoder.explainCode",
        "title": "MaCoder: Explain Code"
      },
      {
        "command": "macoder.refactorCode",
        "title": "MaCoder: Refactor Code"
      },
      {
        "command": "macoder.showInfo",
        "title": "MaCoder: Show Information"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "macoder.explainCode",
          "group": "macoder",
          "when": "editorHasSelection || editorIsOpen"
        },
        {
          "command": "macoder.refactorCode",
          "group": "macoder",
          "when": "editorHasSelection || editorIsOpen"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.60.0",
    "@vscode/test-electron": "^1.5.0",
    "@vscode/vsce": "^2.15.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "license": "MIT"
}